  <script>
            var head=[1,2,2,1];
            var r_head=[];

            for(var i=head.length-1;i>=0;i--)
            {
                r_head.push(head[i]);
            }
            if(JSON.stringify(head)==JSON.stringify(r_head))
            {
                console.log(true);
            }
            else
            {
                console.log(false); 

            }
 </script>
